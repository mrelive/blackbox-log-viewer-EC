{
  "name": "echo-corp-blackbox-decoder",
  "productName": "ECHO CORP Blackbox Decoder",
  "displayName": "ECHO CORP Blackbox Decoder",
  "description": "Echo Corp standalone Blackbox log decoder (CSV/JSON output).",
  "version": "1.0.8",
  "type": "module",
  "main": "index.html",
  "bin": {
    "bbl-decode": "./decoder-entry.cjs"
  },
  "pkg": {
    "scripts": [
      "decoder-entry.cjs"
    ],
    "assets": [
      "bin/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ]
  },
  "files": [
    "decoder-entry.cjs",
    "LICENSE"
  ],
  "default_locale": "en",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src",
    "build:decoder": "node build-decoder.cjs"
  },
  "repository": {
    "type": "git",
    "url": "github.com/betaflight/blackbox-log-viewer"
  },
  "author": "The Betaflight open source project.",
  "license": "GPL-3.0",
  "dependencies": {
    "Leaflet.MultiOptionsPolyline": "hgoebl/Leaflet.MultiOptionsPolyline",
    "bootstrap": "~3.4.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "html2canvas": "^1.0.0-rc.5",
    "jquery": "^3.7.1",
    "jquery-ui": "^1.13.2",
    "leaflet": "^1.9.3",
    "leaflet-marker-rotation": "^0.4.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.1",
    "throttle-debounce": "^5.0.0",
    "vite": "^5.4.21",
    "vite-plugin-pwa": "^0.19.7"
  },
  "devDependencies": {
    "inflection": "1.12.0",
    "yarn": "^1.22.0",
    "esbuild": "^0.21.0"
  },
  "resolutions": {
    "**/**/lodash.template": "^4.5.0"
  },
  "engines": {
    "node": "24.x"
  }
}
